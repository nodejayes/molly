    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "doc": "./node_modules/.bin/typedoc --mode file --module commonjs --target ES6 --readme ./README.md --ignoreCompilerErrors --out ./docs ./src",
        "test": "nyc mocha --recursive -r ts-node/register \"./spec/**/*.spec.ts\"",
        "test:integration": "mocha --bail -r ts-node/register \"./spec/integration/**/*.spec.ts\"",
        "test:unit": "mocha --bail -r ts-node/register \"./spec/unit/**/*.spec.ts\"",
        "test:websocket": "mocha --bail -r ts-node/register \"./spec/integration/websockets.spec.ts\"",
        "release": "tsc && npm publish"
    },
    "dependencies": {
        "body-parser": "^1.18.2",
        "express": "^4.16.2",
        "helmet": "^3.11.0",
        "joi": "^13.1.2",
        "lodash": "^4.17.5",
        "mongodb": "^3.0.2",
        "ws": "^4.1.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.16.8",
        "@types/chai": "^4.1.2",
        "@types/express": "^4.11.1",
        "@types/helmet": "^0.0.37",
        "@types/joi": "^13.0.5",
        "@types/lodash": "^4.14.102",
        "@types/mocha": "^2.2.48",
        "@types/mongodb": "^3.0.5",
        "@types/morgan": "^1.7.35",
        "@types/node": "^9.4.5",
        "@types/ws": "^4.0.1",
        "chai": "^4.1.2",
        "mocha": "^5.0.1",
        "nyc": "^11.4.1",
        "request": "^2.83.0",
        "request-promise": "^4.2.2",
        "ts-node": "^4.1.0",
        "tslint-config-airbnb": "^5.6.0",
        "typedoc": "^0.5.0",
        "typescript": "2.7.1"
    }
}
