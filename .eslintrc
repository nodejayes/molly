
{
    "globals": {
        "Promise": true
    },
    "env": {
        "es6": true,
        "mocha": true,
        "node": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "max-len": ["error", {
            "code": 300,
            "tabWidth": 4,
            "ignoreComments": true,
            "ignoreStrings": true
        }],
        "linebreak-style":"off"
    }
}
    